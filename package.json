    "build:prod": "REACT_APP_FTL_ENV=production react-scripts build",
    "build:bat": "REACT_APP_FTL_ENV=devbat react-scripts build",
    "fbdeploy": "npm run build && firebase use default && firebase deploy",
    "fbdeploy:prod": "npm run build:prod && firebase use production && firebase deploy",
    "fbdeploy:dev-bat": "npm run build:dev-bat && firebase use dev-bat && firebase deploy",
    "pusheverywhere": "npm run fbdeploy && npm run fbdeploy:prod && git push",
    "eject": "react-scripts eject"
  },
